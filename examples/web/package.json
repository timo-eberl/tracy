{
  "name": "tracy",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "node build-wasm.js",
    "build:debug": "node build-wasm.js --debug",
    "watch:c": "nodemon --watch ../../src --watch ../../include --ext c,h --exec \"npm run build\"",
    "watch:c:debug": "nodemon --watch ../../src --watch ../../include --ext c,h --exec \"npm run build:debug\"",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "dev": "concurrently \"npm:watch:c\" \"npm:vite:dev\"",
    "dev:debug": "concurrently \"npm:watch:c:debug\" \"npm:vite:dev\"",
    "dev:docker": "concurrently \"npm:watch:c\" \"vite --host\"",
    "test:native": "cd .. && zig build test --summary all",
    "deploy": "npm run build && vite build",
    "zig_build:wasm": "zig build web -Dbuild-web -Doptimize=ReleaseSmall -Demsdk=$EMSDK",
    "zig_build": "npm run zig_build:wasm && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timo-eberl/tracy.git"
  },
  "author": "Timo Eberl",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/timo-eberl/tracy/issues"
  },
  "homepage": "https://tracy.timoeberl.de",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "live-server": "^1.2.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  }
}
