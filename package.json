{
  "name": "tracy",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "build": "./build.sh",
    "build:debug": "./build_debug.sh",
    "watch:c": "nodemon --watch . --ext c --exec \"npm run build\"",
    "watch:c:debug": "nodemon --watch . --ext c --exec \"npm run build:debug\"",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:preview": "vite preview",
    "dev": "concurrently \"npm:watch:c\" \"npm:vite:dev\"",
    "dev:debug": "concurrently \"npm:watch:c:debug\" \"npm:vite:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timo-eberl/tracy.git"
  },
