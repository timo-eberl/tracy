  "bugs": {
    "url": "https://github.com/timo-eberl/tracy/issues"
  },
  "homepage": "https://github.com/timo-eberl/tracy#readme",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "live-server": "^1.2.0",
    "nodemon": "^3.1.10",
    "vite": "^7.1.6"
  }
}
